{
  "name": "gateway-service",
  "version": "0.0.1",
  "description": "",
  "author": "Ashwin S",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "start:dev": "tsc-watch --onSuccess \"node -r dotenv/config build/src/main.js dotenv_config_path=./.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config build/main.js dotenv_config_path=./.env.test\"",
    "start:prod": "node build/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "_moduleAliases": {
    "application": "build/src/application",
    "common": "build/src/common",
    "modules": "build/src/modules",
    "domain": "build/src/domain",
    "controllers": "build/src/controllers",
    "configuration": "build/src/configuration",
    "libraries": "build/src/libraries",
    "interface": "build/src/interface",
    "middleware": "build/src/middleware",
    "constants": "build/src/constants",
    "src": "build/src"
  },
  "dependencies": {
    "@fastify/static": "^6.6.0",
    "@fastify/swagger": "8.2.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/microservices": "9.2.1",
    "@nestjs/platform-fastify": "9.2.1",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "module-alias": "^2.2.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@nestjs/testing": "8.0.0",
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.20",
    "@types/jest": "26.0.10",
    "@types/node": "14.0.27",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.4.0",
    "mongoose": "5.10.0",
    "prettier": "2.1.2",
    "supertest": "4.0.2",
    "ts-jest": "26.2.0",
    "ts-node": "9.0.0",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.6.3"
  }
}
